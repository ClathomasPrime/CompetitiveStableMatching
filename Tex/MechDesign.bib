@ARTICLE{AshlagiUnbalancedCompetition17,
  title = {Unbalanced Random Matching Markets: The Stark Effect of Competition},
  author = {Ashlagi, Itai and Kanoria, Yash and Leshno, Jacob D.},
  year = {2017},
  journal = {Journal of Political Economy},
  volume = {125},
  number = {1},
  pages = {69 - 98},
  url = {https://EconPapers.repec.org/RePEc:ucp:jpolec:doi:10.1086/689869},
  annotate = {
    The big breakthrough -- being unbalanced has a huge effect on the average
    structure of the core of stable matching markets.
    Studies two statistics -- the average rank of agents for their match,
    and the percentage of agents with multiple stable partners.
    Bounds on the expectation of these statistics, as well as their convergence
    with probability approaching 1, is derived.
  },
}

@article{PittelLikelyStableUnbalanced19,
  author = {Pittel, Boris},
  title = {On Likely Solutions of the Stable Matching Problem with Unequal Numbers of Men and Women},
  journal = {Mathematics of Operations Research},
  volume = {44},
  number = {1},
  pages = {122-146},
  year = {2019},
  doi = {10.1287/moor.2017.0917},
  URL = { https://doi.org/10.1287/moor.2017.0917 },
  eprint = { https://doi.org/10.1287/moor.2017.0917 },
  annotate = {
    Inspired by AshlagiUnbalancedCompetition17, this guy returned to the
    analysis of matching markets. He proves basically tight bounds on the
    expectation of relevant statistics (lots of these are just the less
