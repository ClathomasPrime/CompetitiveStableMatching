@article{RothCollegeRevisited89,
  ISSN = {00129682, 14680262},
  URL = {http://www.jstor.org/stable/1911052},
  author = {Alvin E. Roth and Marilda Sotomayor},
  journal = {Econometrica},
  number = {3},
  pages = {559--570},
  publisher = {[Wiley, Econometric Society]},
  title = {The College Admissions Problem Revisited},
  volume = {57},
  year = {1989},
  abstract = {The college admissions problem is perhaps the simplest model of
    many-to-one matching in two-sided markets such as labor markets. We show
    that the set of stable outcomes (which is equal to the core defined by
    weak domination) has some surprising properties not found in models
    of one-to-one matching. These properties may help to explain the
    success that this kind of model has had in explaining empirical
    observations.
  },
  annotate = {
    Basically, in college-proposing DA, a college can ``kick itself out'' of a
    highly-valued student, cause a different college to propose to the student,
    and end up loosing him/her.
    Also, there exist non-stable matching which are preferred by every college to
    the college-optimal stable matching.
  },
}

